# Header: Options
msgid "OP_Header"
msgstr "Options"

# Group: general settings
msgid "OP_group_general"
msgstr "PARAMÈTRES GÉNÉRAUX"

# Option: UI language
msgid "OP_uiLanguage"
msgstr "Langue de l'interface"

# Tooltip: UI language
msgid "OP_uiLanguage_tooltip"
msgstr "Définit la langue des éléments de l'interface&#xA;comme ce panneau d'options."

# Option: splash scenario name on setup
msgid "OP_showTitleSplash"
msgstr "Afficher titre du scénario"

# Tooltip: splash scenario name on setup
msgid "OP_showTitleSplash_tooltip"
msgstr "Affiche le nom du scénario en fondu pendant 2 secondes&#xA;lors de la mise en place d'un scénario."

# Option: splash token on draw
msgid "OP_showTokenSplash"
msgstr "Afficher le pion agrandi à l'écran"

# Tooltip: splash token on draw
msgid "OP_showTokenSplash_tooltip"
msgstr "Affiche une version agrandie du pion pioché&#xA;pendant 2 secondes."

# Option: Enable all card helpers
msgid "OP_enableCardHelpers"
msgstr "Activer toutes les aides de cartes"

# Tooltip: Enable all card helpers
msgid "OP_enableCardHelpers_tooltip"
msgstr "Active toutes les aides de cartes (généralement activées via le menu contextuel).&#xA;Exemples: Faux Pacte et Livre des mythes vivants"

# Option: Make damage / horror tokens clickable
msgid "OP_makeDamageHorrorClickable"
msgstr "Rendre les pions de dégâts / horreur cliquables"

# Tooltip: Make damage / horror tokens clickable
msgid "OP_makeDamageHorrorClickable_tooltip"
msgstr "Rend les pions de dégâts et d'horreur cliquables&#xA;(en plus de la saisie numérique)."

# Option: Show '1' on damage / horror tokens
msgid "OP_showOneDamageHorror"
msgstr "Afficher un 1 sur les pions de dégâts / horreur"

# Tooltip: Show '1' on damage / horror tokens
msgid "OP_showOneDamageHorror_tooltip"
msgstr "Au lieu de pions vierges (comme ils le sont physiquement),&#xA;ceci ajoutera un '1' à l'état par défaut."

# Group: play area settings
msgid "OP_group_playarea"
msgstr "PARAMÈTRES DE LA ZONE DE JEU"

# Option: play area snap tags
msgid "OP_playAreaSnapTags"
msgstr "Activer les points d'accroche"

# Tooltip: play area snap tags
msgid "OP_playAreaSnapTags_tooltip"
msgstr "Seules les cartes avec le tag 'Lieu' s'accrocheront (les cartes officielles sont supportées par défaut).&#xA;Désactivez si vous avez des problèmes avec du contenu personnalisé."

# Option: location connections
msgid "OP_playAreaConnections"
msgstr "Dessiner les connexions entre lieux"

# Tooltip: location connections
msgid "OP_playAreaConnections_tooltip"
msgstr "Dessine automatiquement les connexions entre les lieux&#xA;en fonction des métadonnées des cartes."

# Option: play area connection color
msgid "OP_playAreaConnectionColor"
msgstr "Choisir la couleur des connexions"

# Tooltip: play area connection color
msgid "OP_playAreaConnectionColor_tooltip"
msgstr "Cette couleur sera utilisée pour dessiner les lignes&#xA;de connexion entre les lieux."

# Option: change custom playarea image on setup
msgid "OP_changePlayAreaImage"
msgstr "Changer le fond à la mise en place"

# Tooltip: change custom playarea image on setup
msgid "OP_changePlayAreaImage_tooltip"
msgstr "Tente de définir une image de fond appropriée&#xA;depuis la galerie d'images de la zone de jeu."

# Group: playermat settings
msgid "OP_group_playermat"
msgstr "PARAMÈTRES PLATEAU JOUEUR"

# Option: enable snap tags
msgid "OP_useSnapTags"
msgstr "Activer les points d'accroche"

# Tooltip: enable snap tags
msgid "OP_useSnapTags_tooltip"
msgstr "Seules les cartes avec le tag 'Soutien' s'accrocheront (les cartes officielles sont supportées par défaut).&#xA;Désactivez si vous avez des problèmes avec du contenu personnalisé."

# Option: show draw 1 button
msgid "OP_showDrawButton"
msgstr "Afficher le bouton \"Piocher 1\""

# Tooltip: show draw 1 button
msgid "OP_showDrawButton_tooltip"
msgstr "Affiche un bouton sous le bouton 'Entretien' qui permet de piocher une carte de votre deck.&#xA;Utile pour le jeu solo à plusieurs mains."

# Option: use class-specific texture
msgid "OP_useClassTexture"
msgstr "Utiliser une texture de classe"

# Tooltip: use class-specific texture
msgid "OP_useClassTexture_tooltip"
msgstr "Contrôle si une texture de plateau joueur spécifique à la classe&#xA;doit être chargée automatiquement."

# Option: use clickable clue-counters
msgid "OP_useClueClickers"
msgstr "Utiliser des compteurs d'indices cliquables"

# Tooltip: use clickable clue-counters
msgid "OP_useClueClickers_tooltip"
msgstr "Au lieu de compter automatiquement les indices dans la zone respective de votre plateau,&#xA;ceci affiche un compteur d'indices cliquable."

# Option: use clickable resource counters
msgid "OP_useResourceCounters"
msgstr "Utiliser des pions ressource cliquables"

# Tooltip: use clickable resource counters
msgid "OP_useResourceCounters_tooltip"
msgstr "Ceci active l'apparition de pions ressource cliquables pour les cartes de joueur.&#xA;(Sélection du Chef = soutiens avec 0 utilisations)"

# Dropdown Option: Resource Counters 1
msgid "OP_useResourceCounters_option1"
msgstr "Activé"

# Dropdown Option: Resource Counters 2
msgid "OP_useResourceCounters_option2"
msgstr "Sélection du Chef"

# Dropdown Option: Resource Counters 3
msgid "OP_useResourceCounters_option3"
msgstr "Désactivé"

# Option: remove a playermat
msgid "OP_removePlayermat"
msgstr "Retirer / Réapparaître un plateau"

# Tooltip: remove a playermat
msgid "OP_removePlayermat_tooltip"
msgstr "Retire un plateau joueur inutilisé pour plus d'espace sur la table.&#xA;Les couleurs affichées sont les couleurs par défaut."

# Dropdown Option: Remove Playermat 1
msgid "OP_removePlayermat_option_select"
msgstr "Cliquer pour sélectionner"

# Dropdown Option: Remove Playermat 2
msgid "OP_removePlayermat_option_white"
msgstr "1: Blanc"

# Dropdown Option: Remove Playermat 3
msgid "OP_removePlayermat_option_orange"
msgstr "2: Orange"

# Dropdown Option: Remove Playermat 4
msgid "OP_removePlayermat_option_green"
msgstr "3: Vert"

# Dropdown Option: Remove Playermat 5
msgid "OP_removePlayermat_option_red"
msgstr "4: Rouge"

# Option: choose a table setup
msgid "OP_tableSetup"
msgstr "Choisir une disposition de table"

# Tooltip: choose a table setup
msgid "OP_tableSetup_tooltip"
msgstr "Choisissez une disposition de table prédéterminée."

# Dropdown Option: Table Setup 1
msgid "OP_tableSetup_option_classic4"
msgstr "Classique (4J)"

# Dropdown Option: Table Setup 2
msgid "OP_tableSetup_option_classic2"
msgstr "Classique (2J)"

# Dropdown Option: Table Setup 3
msgid "OP_tableSetup_option_upright4"
msgstr "Verticale (4J)"

# Dropdown Option: Table Setup 4
msgid "OP_tableSetup_option_sides4"
msgstr "Côtés (4J)"

# Dropdown Option: Table Setup 5
msgid "OP_tableSetup_option_solo"
msgstr "Vrai Solo (1J)"

# Group: accessories
msgid "OP_group_accessories"
msgstr "ACCESSOIRES"

# Option: show attachment helper
msgid "OP_showAttachmentHelper"
msgstr "Aide aux attachements"

# Tooltip: show attachment helper
msgid "OP_showAttachmentHelper_tooltip"
msgstr "Fournit un sac de la taille d'une carte pour les cartes qui sont attachées à d'autres cartes&#xA;(ex. Sac à dos)."

# Option: show CYOA campaign guides
msgid "OP_showCYOA"
msgstr "Guides de Campagne CYOA"

# Tooltip: show CYOA campaign guides
msgid "OP_showCYOA_tooltip"
msgstr "Affiche les guides de campagne redessinés dans un style&#xA;'Livre dont vous êtes le héros'."

# Option: show displacement tool
msgid "OP_showDisplacementTool"
msgstr "Outil de déplacement"

# Tooltip: show displacement tool
msgid "OP_showDisplacementTool_tooltip"
msgstr "Ceci permet de déplacer tous les objets sur la zone de jeu principale&#xA;dans une direction choisie."

# Option: show hand helper
msgid "OP_showHandHelper"
msgstr "Aide pour la main"

# Tooltip: show hand helper
msgid "OP_showHandHelper_tooltip"
msgstr "Ne comptez plus jamais les cartes de votre main ! Cet outil le fait pour vous&#xA;et calcule même la taille maximale de votre main."

# Option: show search assistant
msgid "OP_showSearchAssistant"
msgstr "Assistant de recherche"

# Tooltip: show search assistant
msgid "OP_showSearchAssistant_tooltip"
msgstr "Recherchez rapidement les 3, 6, 9 ou X premières&#xA;cartes de votre deck !"

# Option: Token Arranger settings
msgid "OP_tokenArrangerOdds"
msgstr "Organiseur de Pions: Calcul des probabilités"

# Tooltip: Token Arranger settings
msgid "OP_tokenArrangerOdds_tooltip"
msgstr "Contrôle si l'Organiseur de Pions&#xA;doit calculer les probabilités de pioche des pions."

# Dropdown Option: Token Arranger 1
msgid "OP_tokenArrangerOdds_no"
msgstr "Pas de probabilités"

# Dropdown Option: Token Arranger 2
msgid "OP_tokenArrangerOdds_basic"
msgstr "Basique"

# Dropdown Option: Token Arranger 3
msgid "OP_tokenArrangerOdds_cumulative"
msgstr "Cumulatif"

# Dropdown Option: Token Arranger 4
msgid "OP_tokenArrangerOdds_full"
msgstr "Complet"

# Button: Defaults
msgid "OP_button_defaults"
msgstr "Charger les valeurs par défaut"

# Button: Close
msgid "OP_button_close"
msgstr "Fermer"

# Download Window: Title
msgid  "DW_title"
msgstr ""

# Download Window: Tabs
msgid "DW_tab1"
msgstr "Campagnes Officielles"

msgid "DW_tab2"
msgstr "Scénarios Officiels"

msgid "DW_tab3"
msgstr "Campagnes de Fans"

msgid "DW_tab4"
msgstr "Scénarios de Fans"

msgid "DW_tab5"
msgstr "Cartes Joueur de Fans"
