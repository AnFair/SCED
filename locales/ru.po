# Clean Up Helper: Options
msgid "CUH_title"
msgstr "Options"

msgid "CUH_importTrauma"
msgstr "Import trauma"

msgid "CUH_importTrauma_description"
msgstr ""
"Enables importing trauma values from the campaign log (custom content might give wrong values!).\n"
"Enter players in the campaign log in this order:\n"
"White, Orange, Green, Red."

msgid "CUH_tidyPlayermats"
msgstr "Tidy playermats"

msgid "CUH_tidyPlayermats_description"
msgstr ""
"Controls whether the playermats should get tidied (removal of all cards and "
"tokens)."

msgid "CUH_removeDrawnLines"
msgstr "Remove drawn lines"

msgid "CUH_removeDrawnLines_description"
msgstr "Controls whether all drawn lines should be removed."
