-- button calls respective function in "Global"
function onLoad()
  self.UI.setXmlTable({
    {
      tag = "Button",
      attributes = {
        onClick = "Global/handleStatTrackerClick",
        position = "0 0 -11",
        width = "250",
        height = "250",
        color = "#00000000"
      }
    }
  })
end
